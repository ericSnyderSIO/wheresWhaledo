% Parameter file for 1ch detector

global detParam % initialize global variable

detParam.twin = 35; % length of time to read in (s)

detParam.th = 48; % Detection threshold (counts)

detParam.fc = 20e3; % filter cutoff frequency

detParam.fs = 200e3; % sampling frequency


detParam.minPkDist =  round(30e-3*detParam.fs);


